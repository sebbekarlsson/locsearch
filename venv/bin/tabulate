#!/home/ianertson/workspace/locsearch/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'tabulate','console_scripts','tabulate'
__requires__ = 'tabulate'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('tabulate', 'console_scripts', 'tabulate')()
    )
