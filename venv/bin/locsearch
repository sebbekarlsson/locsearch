#!/home/ianertson/workspace/locsearch/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'locsearch','console_scripts','locsearch'
__requires__ = 'locsearch'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('locsearch', 'console_scripts', 'locsearch')()
    )
